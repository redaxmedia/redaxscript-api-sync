{
	"name": "redaxmedia/redaxscript-api-sync",
	"description": "Yet another cronjob for Redaxscript's website",
	"version": "4.0.0",
	"homepage": "https://redaxscript.com",
	"license": "GPL-3.0",
	"require":
	{
		"php": ">7.0"
	},
	"require-dev":
	{
		"phpdocumentor/phpdocumentor": "2.9.0",
		"redaxmedia/redaxscript": "dev-master"
	},
	"scripts":
	{
		"post-install-cmd":
		[
			"cd vendor/redaxmedia/redaxscript && composer install"
		],
		"post-update-cmd":
		[
			"@post-install-cmd"
		]
	}
}
